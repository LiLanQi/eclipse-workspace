package test;

public class Child {

	static int cakeQuality = 4;
	
	public static void main(String[] args) {
		
		Child xiaoming = new Child();
		System.out.println(xiaoming.cakeQuality);
		Child xiaoqiang = new Child();
		System.out.println(xiaoqiang.cakeQuality);
		System.out.println(xiaoming.cakeQuality);
	}
}
